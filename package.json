{
  "name": "echarts-examples",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npx webpack --config build/webpack.config.js --mode production",
    "watch": "npx webpack --config build/webpack.config.js --mode development --devtool sourcemap --watch",
    "localsite": "npm run build && node build/copyResource.js --env localsite",
    "release": "npm run build && node build/copyResource.js --env asf",
    "dev": "npx concurrently --kill-others \"npm:watch\" \"npm:server\"",
    "build:example": "node tool/build-example.js",
    "server": "node build/server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "argparse": "^1.0.9",
    "babel-loader": "^8.1.0",
    "chalk": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^3.5.3",
    "cwebp-bin": "^6.1.1",
    "file-loader": "^4.3.0",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "gray-matter": "^4.0.2",
    "marked": "^0.3.12",
    "mini-css-extract-plugin": "^0.8.2",
    "node-static": "^0.7.11",
    "open": "^7.1.0",
    "sass.js": "^0.11.1",
    "sassjs-loader": "^2.0.0",
    "sharp": "^0.26.2",
    "style-loader": "^1.2.1",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yargs": "^6.6.0"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "resize-detector": "^0.2.2",
    "scroll-into-view": "^1.14.2",
    "sucrase": "^3.15.0",
    "vanilla-lazyload": "^12.5.1",
    "vue-i18n": "^8.18.2",
    "vue-scrollactive": "^0.9.3"
  }
}
